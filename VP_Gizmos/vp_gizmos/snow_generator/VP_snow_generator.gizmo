))\n\nif nuke.thisNode()\['SnowMass'].value() == \"3\":\n\tnuke.toNode('ParticleFGMassDot').setInput(0, nuke.toNode('ParticleFGMass3'))\n\nif nuke.thisNode()\['SnowMass'].value() == \"5\":\n\tnuke.toNode('ParticleFGMassDot').setInput(0, nuke.toNode('ParticleFGMass5'))\n\nif nuke.thisNode()\['SnowMass'].value() == \"10\":\n\tnuke.toNode('ParticleFGMassDot').setInput(0, nuke.toNode('ParticleFGMass10'))\n\nif nuke.thisNode()\['SnowBGMass'].value() == \"0\":\n\tnuke.toNode('ParticleBGMassDot').setInput(0, nuke.toNode('ParticleBGMass0'))\n\nif nuke.thisNode()\['SnowBGMass'].value() == \"1\":\n\tnuke.toNode('ParticleBGMassDot').setInput(0, nuke.toNode('ParticleBGMass1'))\n\nif nuke.thisNode()\['SnowBGMass'].value() == \"3\":\n\tnuke.toNode('ParticleBGMassDot').setInput(0, nuke.toNode('ParticleBGMass3'))\n\nif nuke.thisNode()\['SnowBGMass'].value() == \"5\":\n\tnuke.toNode('ParticleBGMassDot').setInput(0, nuke.toNode('ParticleBGMass5'))\n\nif nuke.thisNode()\['SnowBGMass'].value() == \"10\":\n\tnuke.toNode('ParticleBGMassDot').setInput(0, nuke.toNode('ParticleBGMass10'))\n\nnuke.message('Snow preset successfully set...')"}
